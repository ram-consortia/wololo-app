    <!-- 评价 -->
    <div class="flex hide">
        <div class="business-firm flex-vertical">
            <div class="account">
                <div class="account-number">给商铺点评</div>
                <span class="close"></span>
            </div>
            <div class="business-center flex-item-vertical">
                <div class="center no-left">
                    <div class="entering">
                        <div class="entering-name">给商铺评分</div>
                        <div class="stars">
                            <!-- 默认空心，加full满心，加half半心 -->
                            <span class="star full"></span>
                            <span class="star half"></span>
                            <span class="star"></span>
                            <span class="star"></span>
                            <span class="star"></span>
                        </div>
                    </div>
                    <div class="entering">
                        <div class="entering-name">你的点评</div>
                        <div class="input-width">
                            <div class="textarea">
                                <textarea type="text" placeholder="请输入商家信息介绍，方便平台进一步审核"></textarea>
                                <p class="number">0/500</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="bottom-button">
                    <span class="quit submint">提交</span>
                </div>
            </div>
        </div>
    </div>
    <!-- 提交成功   -->
    <div class="flex hide">
        <div class="business-firm flex-vertical">
            <div class="account">
                <div class="account-number">评价成功</div>
                <span class="close"></span>
            </div>
            <div class="success">
                <div class="success-img"></div>
                <p class="cessful">评价成功，期待再次为您服务！</p>
            </div>
        </div>
