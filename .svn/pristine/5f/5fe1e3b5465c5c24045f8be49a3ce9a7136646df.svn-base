@import "./wishList";
.swiper-container {
  height: 252px;
  overflow: hidden;
  width: 90%;
  .swiper-slide{
    background: #ccc;
    border-radius: 16px;
    overflow: hidden;
    height: 90%;
  }
  .swiper-pagination-bullet{
    width: 6px;
    height: 6px;
  }
  .swiper-pagination-bullet-active{
    background: #000;
    transform: scale(1.1);
  }
}
.wish_tag{
  z-index: 10;
  position: absolute;
  left: -2px;
  top: 24px;
}

.explore_list{
  border-radius: 12px;
  overflow: hidden;
  box-shadow: 1px 3px 12px #f0f0f0;
  width: 90%;
  margin: 0 auto;
  .exlist_item{
    background: #ffffff;
    display: flex;
    align-items: center;
    padding: 20px;
    border-bottom: 1px solid #eee;
  .item_content{
    height: 48px;
    width: 100%;
    display: flex;
    .logo{
      width: 48px;
      height: 48px;
      img{
        width: 100%;
        height: 100%;
      }
    }
    .desc{
      margin-left: 18px;
      .title{
        font-size: 18px;
        color: #1E1E1E;
        height: 22px;
        margin-top: 4px;
      }
      .available{
        color: #848484;
        margin-top: 4px;
        font-size: 12px;
      }
    }
  }
  }
  .exlist_item:last-child{
    border: none;
  }
}