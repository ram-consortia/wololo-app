*{
    padding: 0;
    margin: 0;
    background-repeat: no-repeat;
}
@font-face {
    font-family: "Proxima Nova";
    src: url("../script/proximanova.ttf");
}
@font-face {
    font-family: "Proxima Nova bold";
    src: url("../script/ProximaNova-Bold.otf");
}
@font-face {
    font-family: "prosemi";
    src: url("../script/ProximaNova-Semibold.otf");
}
html,body{
    width: 100%;
    height: 100%;
    font-family: "Proxima Nova";
    color: #333333;
    background-color: #FBFCFF;
    --primary:#ff852b;
}
.prosemi{
    font-family: prosemi;
}
.primary{
    color: var(--primary);
}
.border_none{
    border: none!important;
}
.bg_fff{
    background-color: #ffffff!important;
}
.bg_red{
    background-color: #F3562F;
}
.bg_grey{
    background-color: #fdfeff;
}
.bg_ccc{
    background-color: #ccc;
}
.bg_blue{
    background-color: #3895FA;
}
.bg_green{
    background-color: #00A861;
}
.color_red{
    color:#DD313C!important;
}
.color_primary{
    color: var(--primary);
}
.color_grey{
    color: #666D72;
}
.color_blue{
    color: #75A7FB;
}
.color_white{
    color: #ffffff;
}
.ellipsis{
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.h100{
    height: 100%;
}
input{
    border: none;
    outline: none;
    display: flex;
    align-items: center;
    padding-bottom: 5px;
    width: 100%;
    background: none;
}
.scroll_y{
    overflow-y: auto;
}
.page{
    display: flex;
    flex-direction: column;
    height: 100%;
    box-sizing: border-box;
    width: 100%;
    padding: 20px 0;
    overflow-x: hidden;
}
.grey{
    color: #848484;
    font-size: 16px;
    margin-top: 10px;
}
.color_grey{
    color: #848484;
}
.size24{
