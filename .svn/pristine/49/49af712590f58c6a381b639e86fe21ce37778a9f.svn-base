<script type="text/html" id="wrap-template">
    <div class="user">


        {{if codeY === 1}}


        <div class="userTopA userTopAB">
            <div class="userTopW">
                <span class="userTopWSpan userTopWSpanA" tapmode="hover" onclick="fnClose();"></span>
                <p class="topRight"  tapmode="hover" onclick="fnToOrder();">
                    Voucher Redeemed
                </p>
            </div>
            <div class="contentFloat contentFloatA" >
                <div class="contentFloatTop">
                    <div class="userLeft">
                        <div data-img-url="{{_user.avatar}}" data-img-type="!avatar" class="contentFloatTopUser" tapmode="hover" onclick="fnOpen('my/personalData_win.html')"></div>

                        <div class="contentFloatTopName">
                            <p class="contentFloatTopNameTop">{{_user | fnShowUserNickname_base}}</p>

                            <p class="contentFloatTopNameFooter">
                                <i></i>
                                <span>{{ {'target': _user.grade, 'key': 'id', 'resKey': 'name', 'data': USER_GRADE} | fnGetRelation_public }}</span>
                            </p>
                        </div>
                    </div>

                </div>

                <div class="contentFloatBottom contentFloatBottomA">

                    <div class="succes" >

                        <img src="../../image/fail_new.png.png">
                        <p>Successful</p>
                    </div>
                    <div class="Reason">
                        <span>[Reason]</span>
                        <span>*Allready redeemed/ time limited </span>
                    </div>
                </div>

            </div>
            <div class="butt">Close</div>
        </div>
    {{else}}

    <div class="userTopA userTopAB">
        <div class="userTopW">
            <span class="userTopWSpan userTopWSpanA" tapmode="hover" onclick="fnClose();"></span>
            <p class="topRight"  tapmode="hover" onclick="fnToOrder();">
                Voucher Redeemed
            </p>
        </div>
        <div class="contentFloat contentFloatA" >
            <div class="contentFloatTop">
                <div class="userLeft">
                    <div data-img-url="{{_user.avatar}}" data-img-type="!avatar" class="contentFloatTopUser" tapmode="hover" onclick="fnOpen('my/personalData_win.html')"></div>

                    <div class="contentFloatTopName">
                        <p class="contentFloatTopNameTop">{{_user | fnShowUserNickname_base}}</p>

                        <p class="contentFloatTopNameFooter">
                            <i></i>
                            <span>{{ {'target': _user.grade, 'key': 'id', 'resKey': 'name', 'data': USER_GRADE} | fnGetRelation_public }}</span>
                        </p>
                    </div>
                </div>

            </div>

            <div class="contentFloatBottom contentFloatBottomA">

                <div class="succes" >

                    <img src="../../image/success_new.png">
                    <p>Successful</p>
                </div>
            </div>

        </div>
        <div class="butt">Close</div>
    </div>

    {{/if}}
</script>
</body>
<script type="text/javascript" src="../../framework/jquery.min.js"></script>
<script type="text/javascript" src="../../framework/jquery-qrcode-0.17.0.min.js"></script>
<script type="text/javascript" src="../../framework/html2canvas.min.js"></script>
<script type="text/javascript" src="../../framework/template-web.js"></script>
<script type="text/javascript" src="../../framework/zepto.min.js"></script>
